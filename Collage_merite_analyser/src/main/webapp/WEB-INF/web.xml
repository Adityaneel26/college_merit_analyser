<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>ProcessRankServlet</servlet-name>
        <servlet-class>com.mycompany.mavenproject2.ProcessRankServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadIndexPageServlet</servlet-name>
        <servlet-class>com.mycompany.mavenproject2.LoadIndexPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProcessRankServlet</servlet-name>
        <url-pattern>/ProcessRankServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadIndexPageServlet</servlet-name>
        <url-pattern>/LoadIndexPageServlet</url-pattern>
    </servlet-mapping>
   <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>

    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
